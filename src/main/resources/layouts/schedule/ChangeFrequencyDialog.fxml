<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER" maxHeight="150.0" maxWidth="300.0" prefHeight="150.0" prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="it.unibo.wastemaster.controller.schedule.ChangeFrequencyDialogController">
   <children>
      <Label text="Change Pickup Frequency">
         <VBox.margin>
            <Insets top="10.0" bottom="10.0" />
         </VBox.margin>
      </Label>
      <ComboBox fx:id="frequencyComboBox" prefWidth="150.0" />
      <Region VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false"  onAction="#handleSave" text="Save"  />
         </children>
         <padding>
            <Insets top="0" right="10.0" bottom="10.0" left="0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets top="10" right="10" bottom="10" left="10" />
   </padding>
</VBox>


